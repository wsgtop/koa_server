{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "启动调试",
      "program": "${workspaceFolder}/app.js",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },

    {
      "name": "附加到进程debug",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },

  ]
}